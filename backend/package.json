{
  "name": "ura-backend",
  "version": "4.0.0",
  "type": "module",
  "scripts": {
    "start": "tsx src/server.ts",
    "dev": "tsx --watch src/server.ts",
    "start:js": "node src/server.js",
    "dev:js": "node --watch src/server.js",
    "typecheck": "tsc --noEmit",
    "test": "cd .. && tsx test-all.ts",
    "sync": "tsx sync.js",
    "sync:status": "tsx sync.js --status",
    "db:migrate": "psql $DATABASE_URL < src/db/migrations/0001_initial.sql",
    "db:seed": "tsx src/db/seed.ts",
    "db:generate": "drizzle-kit generate",
    "db:studio": "drizzle-kit studio",
    "worker": "tsx src/services/ingestion.worker.ts"
  },
  "dependencies": {
    "axios": "^1.7.0",
    "bcryptjs": "^3.0.3",
    "bullmq": "^5.69.3",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.4.0",
    "drizzle-orm": "^0.45.1",
    "drizzle-zod": "^0.8.3",
    "express": "^4.18.2",
    "ioredis": "^5.9.3",
    "jsonwebtoken": "^9.0.3",
    "pg": "^8.18.0",
    "pino": "^10.3.1",
    "pino-pretty": "^13.1.3",
    "prom-client": "^15.1.3",
    "zod": "^3.24.0"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/compression": "^1.7.5",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^22.10.0",
    "@types/pg": "^8.16.0",
    "drizzle-kit": "^0.31.9",
    "tsx": "^4.19.0",
    "typescript": "^5.7.0"
  }
}
